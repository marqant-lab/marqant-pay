{
    "name": "marqant-lab/marqant-pay",
    "description": "A flexible payment provider implementation by Marqant.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Venturini",
            "email": "thomas@du.digital"
        }
    ],
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0",
        "cviebrock/eloquent-sluggable": "^6.0|^7.0|^8.0",
        "i-rocky/eloquent-dynamic-relation": "^1.0",
        "moneyphp/money": "^3.2",
        "php": "^7.4",
        "spatie/data-transfer-object": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Marqant\\MarqantPay\\Seeds\\": "database/seeds",
            "Marqant\\MarqantPay\\Tests\\": "tests",
            "Marqant\\MarqantPay\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marqant\\MarqantPay\\MarqantPayServiceProvider"
            ],
